<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<!--  Maven Central  -->
	<groupId>com.github.aiyoyoyo</groupId>
	<artifactId>jees-jdbs</artifactId>
	<version>1.1.4-SNAPSHOT</version>
	<name>Java Database Support</name>

	<description>Spring Atomikos database supprot</description>
	<url>${jees.url}</url>

	<!-- Project Information -->
	<properties>
		<main.basedir>${basedir}/..</main.basedir>
		<jees.artifactId>jees-jdbs</jees.artifactId>
	</properties>

	<parent>
		<groupId>com.github.aiyoyoyo</groupId>
		<artifactId>jees-boot</artifactId>
		<version>1.1.4-SNAPSHOT</version>
		<relativePath>../jees-boot/pom.xml</relativePath>
	</parent>
	<dependencies>
		<!-- Jees -->
		<dependency>
			<groupId>com.github.aiyoyoyo</groupId>
			<artifactId>jees-jtls</artifactId>
			<version>${jees.jtls.version}</version>
		</dependency>
		<!--Spring Boot -->
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-jta-atomikos</artifactId>
			<version>${spring.boot.version}</version>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-data-jpa</artifactId>
			<version>${spring.boot.version}</version>
		</dependency>
		<!-- Test -->
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-test</artifactId>
			<version>${spring.boot.version}</version>
		</dependency>
		<!-- MySQL -->
		<dependency>
			<groupId>mysql</groupId>
			<artifactId>mysql-connector-java</artifactId>
			<version>${mysql.version}</version>
		</dependency>
		<!-- Lombok -->
		<dependency>
			<groupId>org.projectlombok</groupId>
			<artifactId>lombok</artifactId>
			<version>${lombok.version}</version>
			<scope>provided</scope>
		</dependency>
	</dependencies>
</project>